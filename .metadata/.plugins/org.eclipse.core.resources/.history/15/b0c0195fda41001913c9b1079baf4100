package Q05;

public class Retangulo implements FormaGeometrica{
	private double lado;
	
	public Retangulo(double lado) {
		super();
		this.lado = lado;
	}

	@Override
	public double area() {
		return this.lado**2;
	}

	@Override
	public double perimetro() {
		// TODO Auto-generated method stub
		return 0;
	}

}
