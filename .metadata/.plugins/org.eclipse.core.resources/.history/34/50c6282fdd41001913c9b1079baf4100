package Q05;

import java.util.Vector;

public class App {
	public static void main(String[] args) {
		
		
		Retangulo r1 = new Retangulo(10, 2.5);
		Retangulo r2 = new Retangulo(5, 2.5);
		
		Quadrado q1 = new Quadrado(5);
		Quadrado q2 = new Quadrado(4);
		
		Circulo c1 = new Circulo(2.5);
		Circulo c2 = new Circulo(4.5);
		Vector<FormaGeometrica> v = new Vector<FormaGeometrica>();
		v.add(r1);
		v.add(r2);
		v.add(q1);
		v.add(q2);
		v.add(c1);
		v.add(c2);
		
		System.out.print("Lados: ");
		
		for (FormaGeometrica f : v) {
			
		}
		
	}
}
