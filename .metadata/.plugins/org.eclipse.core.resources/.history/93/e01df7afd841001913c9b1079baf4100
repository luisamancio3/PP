package Q04;

public class App {
	public static void main(String[] args) {
		Bits b1 = new Bits(10);
		Bits b2 = new Bits(10);	
		
		for (int i = 0; i < 10; i++) {
			b1.setBit('2');
		}

		for (int i = 10; i < 0; i--) {
			b2.setBit((char) i);
		}
		
		System.out.println(b1.getBits());
//		System.out.println(b2.toString());
	}
}
