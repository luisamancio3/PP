package Q05;

public class Quadrilatero implements FormaGeometrica{
	private int lado1, lado2, lado3, lado4;

	public Quadrilatero(int lado1, int lado2, int lado3, int lado4) {
		super();
		this.lado1 = lado1;
		this.lado2 = lado2;
		this.lado3 = lado3;
		this.lado4 = lado4;
	}

	@Override
	public int area() {
		return this.lado1 * this.lado2 * this.lado3 * this.lado4;
	}

	@Override
	public int perimetro() {
		// TODO Auto-generated method stub
		return 0;
	}
	
}
